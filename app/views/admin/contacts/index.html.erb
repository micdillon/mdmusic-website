<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Bio</th>
    <th>Phone number</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% debug @admin_contacts %>
<% @admin_contacts.each do |admin_contact| %>
  <tr>
    <td><%= admin_contact.bio %></td>
    <td><%= admin_contact.phone %></td>
    <td><%= admin_contact.email %></td>
    <td><%= link_to 'Show', admin_contact_path(admin_contact) %></td>
    <td><%= link_to 'Edit', edit_admin_contact_path(admin_contact) %></td>
    <td><%= link_to 'Destroy', admin_contact_path(admin_contact), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_admin_contact_path %>
